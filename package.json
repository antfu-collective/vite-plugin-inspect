{
  "name": "vite-plugin-inspect",
  "version": "0.3.11",
  "description": "Inspect the intermediate state of Vite plugins",
  "keywords": [
    "vite-plugin"
  ],
  "homepage": "https://github.com/antfu/vite-plugin-inspect#readme",
  "bugs": {
    "url": "https://github.com/antfu/vite-plugin-inspect/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vite-plugin-inspect.git"
  },
  "funding": "https://github.com/sponsors/antfu",
  "license": "MIT",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rimraf dist && run-s build:*",
    "dev": "vite src/client",
    "build:js": "unbuild",
    "build:client": "vite build src/client",
    "dev:js": "nr build:js --watch src",
    "dev:client": "vite build src/client --watch",
    "release": "bumpp --commit --push --tag && npm publish",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "sirv": "^2.0.0",
    "ufo": "^0.7.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.14.2",
    "@antfu/ni": "^0.12.0",
    "@antfu/utils": "^0.4.0",
    "@iconify/json": "^2.0.17",
    "@rollup/pluginutils": "^4.1.2",
    "@types/codemirror": "^5.60.5",
    "@types/debug": "^4.1.7",
    "@types/diff-match-patch": "^1.0.32",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.5",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vueuse/core": "^7.4.3",
    "@vueuse/router": "^7.4.3",
    "bumpp": "^7.1.1",
    "codemirror": "^5.65.0",
    "codemirror-theme-vars": "^0.1.1",
    "diff-match-patch": "^1.0.5",
    "eslint": "^8.5.0",
    "eslint-plugin-jest": "^25.3.2",
    "esno": "^0.13.0",
    "fuse.js": "^6.5.3",
    "kolorist": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "prism-theme-vars": "^0.2.2",
    "rimraf": "^3.0.2",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4",
    "unbuild": "^0.6.7",
    "unplugin-auto-import": "^0.5.5",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.11",
    "vis-data": "^7.1.2",
    "vis-network": "^9.1.0",
    "vite": "^2.7.10",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-pages": "^0.19.8",
    "vite-plugin-windicss": "^1.6.1",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
