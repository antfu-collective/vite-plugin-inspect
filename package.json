{
  "name": "vite-plugin-inspect",
  "version": "0.7.28",
  "packageManager": "pnpm@8.5.1",
  "description": "Inspect the intermediate state of Vite plugins",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vite-plugin-inspect#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vite-plugin-inspect.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/vite-plugin-inspect/issues"
  },
  "keywords": [
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && run-s build:*",
    "build:client": "vite build src/client",
    "build:js": "unbuild",
    "dev": "nr stub && cross-env INSPECT_DEV=true vite src/client",
    "dev:client": "vite build src/client --watch",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "@rollup/pluginutils": "^5.0.2",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "open": "^9.1.0",
    "picocolors": "^1.0.0",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@antfu/ni": "^0.21.3",
    "@iconify/json": "^2.2.68",
    "@types/codemirror": "^5.60.7",
    "@types/debug": "^4.1.8",
    "@types/diff-match-patch": "^1.0.32",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.2.3",
    "@unocss/eslint-config": "^0.52.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vueuse/core": "^10.1.2",
    "@vueuse/router": "^10.1.2",
    "bumpp": "^9.1.0",
    "codemirror": "^5.65.8",
    "codemirror-theme-vars": "^0.1.2",
    "comlink": "^4.4.1",
    "cross-env": "^7.0.3",
    "diff-match-patch": "^1.0.5",
    "echarts": "^5.4.2",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "fuse.js": "^6.6.2",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "ohash": "^1.1.2",
    "prism-theme-vars": "^0.2.4",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "splitpanes": "^3.1.5",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "unocss": "^0.52.2",
    "unplugin-auto-import": "^0.16.2",
    "unplugin-vue-components": "^0.24.1",
    "vis-data": "7.1.6",
    "vis-network": "9.1.6",
    "vite": "^4.3.8",
    "vite-dev-rpc": "^0.1.2",
    "vite-hot-client": "^0.2.1",
    "vite-plugin-pages": "^0.30.1",
    "vue": "^3.3.4",
    "vue-echarts": "^6.5.5",
    "vue-router": "^4.2.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
