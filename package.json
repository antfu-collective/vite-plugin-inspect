{
  "name": "vite-plugin-inspect",
  "type": "module",
  "version": "11.0.0",
  "packageManager": "pnpm@10.7.1",
  "description": "Inspect the intermediate state of Vite plugins",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vite-plugin-inspect#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vite-plugin-inspect.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/vite-plugin-inspect/issues"
  },
  "keywords": [
    "vite-plugin"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./nuxt": "./dist/nuxt.mjs",
    "./*": "./*"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && pnpm run --sequential /^build:/",
    "build:client": "vite build src/client",
    "build:js": "unbuild",
    "dev": "nr stub && INSPECT_DEV=true vite src/client",
    "dev:client": "vite build src/client --watch",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish"
  },
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    }
  },
  "dependencies": {
    "ansis": "^3.17.0",
    "debug": "^4.4.0",
    "error-stack-parser-es": "^1.0.5",
    "ohash": "^2.0.11",
    "open": "^10.1.0",
    "perfect-debounce": "^1.0.0",
    "sirv": "^3.0.1",
    "unplugin-utils": "^0.2.4",
    "vite-dev-rpc": "^1.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/ni": "^24.3.0",
    "@antfu/utils": "^9.2.0",
    "@iconify/json": "^2.2.323",
    "@nuxt/kit": "^3.16.2",
    "@types/codemirror": "^5.60.15",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.14.0",
    "@unocss/eslint-config": "^66.0.0",
    "@unocss/eslint-plugin": "^66.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.5.13",
    "@vueuse/core": "^13.0.0",
    "@vueuse/router": "^13.0.0",
    "bumpp": "^10.1.0",
    "codemirror": "^5.65.16",
    "codemirror-theme-vars": "^0.1.2",
    "comlink": "^4.4.2",
    "diff-match-patch-es": "^1.0.1",
    "echarts": "^5.6.0",
    "eslint": "^9.23.0",
    "floating-vue": "^5.2.2",
    "fuse.js": "^7.1.0",
    "lint-staged": "^15.5.0",
    "pathe": "^2.0.3",
    "pinia": "^3.0.1",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.12.1",
    "splitpanes": "^4.0.3",
    "typescript": "~5.8.2",
    "unbuild": "^3.5.0",
    "unocss": "^66.0.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.12.0",
    "vis-data": "7.1.7",
    "vis-network": "9.1.6",
    "vite": "^6.2.5",
    "vite-hot-client": "^2.0.4",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  },
  "resolutions": {
    "vite": "^6.2.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
